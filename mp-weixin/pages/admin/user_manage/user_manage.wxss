.rank_wrap {
  margin: 30rpx;
  border: 1rpx solid #fff;
  padding: 20rpx 0;
  position: relative;
}
.rank_wrap .rank_title {
  font-weight: 700;
  margin: 20rpx 0;
  font-size: 34rpx;
  text-align: center;
}
.rank_wrap .rank_main .rank_main_title {
  margin: 20rpx 0 !important;
  font-size: 32rpx !important;
}
.rank_wrap .rank_main .rank_main_list {
  display: flex;
  font-size: 28rpx;
  justify-content: space-around;
  padding: 10rpx 0;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}
.rank_wrap .rank_main .rank_main_list view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 20%;
}
.rank_wrap .rank_main .rank_main_list button {
  margin: 0;
  font-size: 28rpx;
  padding: 0;
  line-height: auto;
}
.rank_wrap .rank_main .rank_main_list .oper_btns {
  display: flex;
  justify-content: space-between;
}
.rank_wrap .row_wrap {
  display: flex;
}
.rank_wrap .row_wrap .row_data {
  margin-left: 10%;
  margin-bottom: 20rpx;
}
.rank_wrap .result {
  position: absolute;
  right: 7%;
  top: 30%;
  font-size: 40rpx;
  font-weight: 700;
}
